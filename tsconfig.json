{
  "include": [
    "src/**/*",
    "prisma/seed.ts"
  ],
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "es6"
    ],
    "allowJs": true,
    "outDir": "./build",
    "strict": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "rootDir": "./",
    "paths": {
      "~/*": [
        "./src/*"
      ]
    }
  }
}